struct Rectangle {

    var width:  int64
    var height: int64

    #[constructor]
    function new (width: in64 height: int64) {

        this.width  = width
        this.height = height

        return this
    }

    function area(this) {

        return this.width * this.height

    }

}

function main(args: str[]): int16 {

    var rect: Rectangle = Rectangle.new(10,15)

    var x: str = "Hello world!"

    println(x) -- This is a comment

    println(str(rect.area()))

    return 0

}