struct Rectangle {

    width  = 0,
    height = 0,

}

function Rectangle:new(width, height) {

    rect = Rectangle

    rect.width  = width
    rect.height = height

    return rect

}

function Rectangle:area() {

    return this.width * this.height

}

function main(args) {

    rect = Rectangle:new(10, 15)

    local x = "Hello world!"

    println(x) -- This is a comment

    println(str(rect:area()))

    return 0

}