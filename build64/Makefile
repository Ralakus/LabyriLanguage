# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Language_config = debug
endif
ifeq ($(config),release)
  Language_config = release
endif

PROJECTS := Language

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Language:
ifneq (,$(Language_config))
	@echo "==== Building Language ($(Language_config)) ===="
	@${MAKE} --no-print-directory -C . -f Language.make config=$(Language_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Language.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Language"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"